{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "node dist/index.js",
    "check": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "db:push": "drizzle-kit push",
    "db:seed": "npm run seed:protocols && npm run seed:medications && npm run seed:chapters && npm run seed:flashcards && npm run seed:nremt",
    "seed:protocols": "tsx server/seed-clark-county-protocols.ts",
    "seed:medications": "tsx server/seed-enhanced-medications.ts",
    "seed:chapters": "tsx server/seed-missing-chapters.ts",
    "seed:flashcards": "tsx server/seed-flashcards-clean.ts",

    "seed:nremt": "tsx server/seed-nremt-questions.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "devDependencies": {
    "vite": "^7.0.4"

    "seed:nremt": "tsx server/seed-nremt-questions.ts"
  },
  "devDependencies": {
    "tsx": "^3.12.7",
    "esbuild": "^0.19.12",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "drizzle-kit": "^0.20.17"

  }
}
